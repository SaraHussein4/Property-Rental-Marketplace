@model List<PropertyRentalDAL.Models.PropertyType>

@{
    ViewData["Title"] = "Default";
}


 <div class="row">
    @foreach(var p in Model){
        <div class="col-sm-6 col-md-4 col-lg-3">
            <div class="item row p-3 rounded-4">
                <div class="box1 h-100 col-4 rounded-start-3 d-flex align-items-center justify-content-center">
                    <i class="fa-solid fa-check"></i>
                </div>
                <div class="box2 h-100 col-8 d-flex align-items-center px-4 rounded-end-3">
                    @p.Name
                </div>
            </div>
        </div>
    }
</div>
